{
  "compilerOptions": {
    "jsx": "react-jsx",          
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "types": ["react", "react-dom"], 
    "verbatimModuleSyntax": false    
  }
}
